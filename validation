    private CompanyDTO findInformationCompany(PreRootDTO companyData, AttachmentsDTO attachments) {
        CompanyDTO company = companyMapperINPI.rootDTOToCompanyDto(companyData.getFormality());
        Optional<LegalFormINPIMatching> legalFormINPIMatching = legalFormINPIMatchingRepository.findById(Integer.valueOf(company.getLegalForm()));
        company.setLegalForm(legalFormINPIMatching.get().getLabel());
        company.setIntracommunityVATNumber("FR" + cleTVA(company.getIdentifier()) + company.getIdentifier());
        for (RepresentativeDTO representativeDTO : company.getRepresentatives()) {
            if (representativeDTO.getLegalEntity() != null) {
                if (representativeDTO.getLegalEntity().getLegalForm() != null) {
                    Optional<LegalFormINPIMatching> legalFormINPIMatchingRepresentatives = legalFormINPIMatchingRepository.findById(Integer.valueOf(representativeDTO.getLegalEntity().getLegalForm()));
                    representativeDTO.getLegalEntity().setLegalForm(legalFormINPIMatchingRepresentatives.get().getLabel());
                }
            }
        }
        if (attachments != null) {
            attachmentsMapperINPI.attachmentsToDocumentsDTO(company, attachments);
        }
        return company;
    }


