updateLiquibase:
  stage: liquibase
  before_script: []
  environment: $GITLAB_ENV
  variables:
    LIQUIBASE_COMMAND_CHANGELOG_FILE: "db.changelog-master.yaml"
    LIQUIBASE_SEARCH_PATH: "./src/main/resources/db/changelog/"
  script:
    - liquibase update
  when: manual
  tags:
    - "$GITLAB_TAG"

